{
  "elements": {
    "scrollable_renderer": {
      "type": "put_scrollable_renderer",
      "x": "${width}/2-228",
      "y": "(${height}-${scrollable_renderer.height})/2 + 31",
      "width": 200,
      "height": 222,
      "elements": {
        "text": {
          "type": "draw_multiline_string",
          "component": {"translate": "bedrock4j.menu.how_to_play.nether-portal_message"},
          "width": 176
        },
        "text1": {
          "type": "draw_multiline_string",
          "component": {"translate": "bedrock4j.menu.how_to_play.nether-portal_message_1"},
          "width": 176
        }
      }
    },
    "panel": {
      "type":"blit_sprite",
      "sprite":"bedrock4j:tiles/small_panel",
      "width":262,
      "height":140,
      "x":"${width}/2-15",
      "y":"(${height}-${panel.height})/2+20"
    },
    "frame": {
      "type":"blit_sprite",
      "sprite":"bedrock4j:tiles/square_recessed_panel",
      "width":"${panel.width}-14",
      "height":"${panel.height}-14",
      "x":"${panel.x}+7",
      "y":"${panel.y}+7"
    },
    "portal": {
      "type":"blit_sprite",
      "sprite": "bedrock4j:container/how_to_play/nether-portal",
      "x":"${panel.x}+8",
      "y":"${panel.y}+8",
      "width":"${panel.width}-16",
      "height":"${panel.height}-16"
    }
  }
}