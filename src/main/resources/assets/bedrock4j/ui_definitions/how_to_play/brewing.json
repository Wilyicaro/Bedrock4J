      "width": 200,
      "height": 222,
      "elements": {
        "text": {
          "type": "draw_multiline_string",
          "component": {"translate": "bedrock4j.menu.how_to_play.brewing_message"},
          "width": 176
        },
        "text1": {
          "type": "draw_multiline_string",
